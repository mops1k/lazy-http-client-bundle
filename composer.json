{
  "name": "mops1k/lazy-http-client-bundle",
  "description": "Bundle for making async `lazy` http requests",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Aleksandr Kvintilyanov",
      "email": "bednyj.mops@gmail.com"
    }
  ],
  "keywords": [ "symfony", "http", "client", "lazy", "api", "guzzle" ],
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.0",
    "ocramius/proxy-manager": "^2.2",
    "symfony/framework-bundle": "~4.1|~4.2|~4.3"
  },
  "autoload": {
    "psr-4": {
      "LazyHttpClientBundle\\": "."
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev"
